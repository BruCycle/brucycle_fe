<br><br>
<center>
  <%= form_with url: "/user", method: :patch, local: true do |f| %>
    <%= f.label :username, "Username", class: 'font-weight-bold text-white' %>
    <%= f.text_field :username, value: current_user.username %>
    <%= f.submit "Update", class: 'btn btn-warning' %>
  <% end %>
</center>